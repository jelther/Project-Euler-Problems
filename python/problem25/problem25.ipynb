    "def fibonacci(n):\n",
    "    if (n in (1,2)):\n",
    "        return 1\n",
    "    \n",
    "    if (n == 0):\n",
    "        return 0\n",
    "    \n",
    "    return fibonacci(n - 1) + fibonacci(n - 2)\n",
    "    "
   ]
