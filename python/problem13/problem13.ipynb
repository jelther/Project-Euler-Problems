    "import operator\n",
    "\n",
    "filename = \"number\"\n",
    "\n",
    "n = []\n",
    "\n",
    "f = open(filename,\"r+\")\n",
    "\n",
    "for line in f:    \n",
    "    n.append(line.splitlines()[0])\n",
    "    \n",
    "total = reduce(operator.add,[int(x) for x in n])\n",
    "\n",
    "print str(total)[0:10]"
   ]
  }
